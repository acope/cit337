package edu.oakland.helper.problem3;

  class Building {
   Building() {  
    	System.out.print("b ");  
   }//Building()
   Building(String name) {
      this();   
      System.out.print("bn " + name);
   }//Building(String name)
  }//class Building
  
  public class House extends Building {
   House() {  
   	System.out.print("h ");  
   }//House()
   
   House(String name) {
     this();   
     System.out.print("hn " + name);
   }//House(String name)
   
   public static void main(String[] args) { 
   	new House("x "); 
   }//main
  }//public class House extends Building